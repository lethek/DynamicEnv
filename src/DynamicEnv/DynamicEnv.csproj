<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <RepositoryUrl>https://github.com/lethek/DynamicEnv</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <Copyright>Copyright Â© Michael Monsour 2020</Copyright>
    <Company />
    <Authors>Michael Monsour</Authors>
    <Product>DynamicEnv</Product>
    <PackageId>DynamicEnv</PackageId>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageProjectUrl>https://github.com/lethek/DynamicEnv</PackageProjectUrl>
    <PackageTags>dynamicenv, dynamic, environment, env, variables</PackageTags>
    <Description>.NET Standard 2.0 library that exposes environment variable interactions through a Dynamic object.</Description>
    <GeneratePackageOnBuild>false</GeneratePackageOnBuild>
    <Version>1.0.0</Version>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.SourceLink.GitHub" Version="1.0.0">
      <PrivateAssets>all</PrivateAssets>
      <IncludeAssets>runtime; build; native; contentfiles; analyzers; buildtransitive</IncludeAssets>
    </PackageReference>
  </ItemGroup>

</Project>

